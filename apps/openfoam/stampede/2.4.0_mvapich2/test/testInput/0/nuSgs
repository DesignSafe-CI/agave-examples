FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuSgs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.5E-5;

boundaryField
{
    object
    {
        type            zeroGradient;
    }

    inlet
    {
        type           calculated;
    }

    outlet
    {
        type           calculated;
    }

#include "boundary"
}

